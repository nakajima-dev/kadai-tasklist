<header class="mb-4">
  <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
    <!--<a class="navbar-brand" href="toppages/index">Tasklists</a>-->
    <%= link_to 'Tasklists', root_path, class: 'navbar-brand' %>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse justify-content-end" id="navbarSupportedContent">
      <ul class="navbar-nav">
        <% if logged_in? %>
          <li class="nav-item"><%= link_to 'Log out', logout_path, method: :delete %></li>
        <% else %>
          <li class="nav-item"><%= link_to 'Sign up', signup_path, class: 'nav-link' %></li>
          <li class="nav-item"><%= link_to 'Log in', login_path, class: 'nav-link' %></li>
        <% end %>
        
      </ul>
    </div>
  </nav>
</header>